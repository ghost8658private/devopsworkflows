Name: Mirror to public

on: 
  workflow_dispatch

jobs:
  mirror_branch_to_public:
    uses: workflow to run
    with:
      source-repo-name: 
      source-branch: dlk
      target-repo-name:
      target-repo-url:
      base-branch:
      main-sha:
      tmo-main-sha:
    secrets:
      ACCESS_TOKEN: ${{ secrets.ACCESS_TOKEN }}